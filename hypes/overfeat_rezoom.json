{
    "data": {
        "train_idl": "./train-list.json",
        "test_idl": "./test-list.json",
        "truncate_data": false
    },
    "logging": {
        "display_iter": 50,
        "save_iter": 10000
    },
    "solver": {
        "opt": "RMS",
        "use_jitter": true,
        "rnd_seed": 1,
        "epsilon": 0.00001, 
        "learning_rate": 0.001, 
        "learning_rate_step": 33000, 
        "hungarian_iou": 0.25,
        "weights": "",
        "head_weights": [1.0, 0.1]
    },
    "use_lstm": false,
    "use_rezoom": true,
    "biggest_box_px": 500,
    "rezoom_change_loss": "center",
    "rezoom_w_coords": [-0.25, 0.25],
    "rezoom_h_coords": [-0.25, 0.25],
    "reregress": true,
    "focus_size": 1.8,
    "early_feat_channels": 256,
    "later_feat_channels": 832,
    "avg_pool_size": 5,
    "num_lstm_layers": 2,
    "image_width": 1024,
    "image_height": 768,
    "grid_height": 24,
    "grid_width": 32,
    "batch_size": 1, 
    "region_size": 32,
    "clip_norm": 1.0,
    "lstm_size": 500,
    "deconv": false,
    "num_classes": 2,
    "rnn_len": 1
}
